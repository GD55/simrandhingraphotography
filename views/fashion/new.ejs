<% include ../partials/header %>
<div class="container">
	<h1 class="heading">New Fashion Image</h1>
	<div class="form">
		<form action="/fashion" method="POST" enctype="multipart/form-data">
			<div class="form-group">
				<label>Order :</label>
				<input class="form-control" type="number" name="order" placeholder="order" required>
			</div>
			<div class="form-group">
				<label>File :</label>
				<input type="file" aria-describedby="inputGroupFileAddon01" name="photo" accept="image/*" required>
			</div>
			<input class="btn btn-lg btn-primary btn-block" type="submit" name="">
		</form>
	</div>
</div>
<% include ../partials/footer %>