<% include ../partials/header %>
<div class="container">
	<h1 class="heading">Edit Order <%= portraitImage.order%></h1>
	<div class="form">
		<form action="/portrait/<%= portraitImage._id %>?_method=PUT" method="POST">
			<div class="form-group">
				<label>Image :</label>
				<input class="form-control" type="text" name="portraitImage[img]" value="<%=portraitImage.img%>">
			</div>
			<div class="form-group">
				<label>Order :</label>
				<input class="form-control" type="number" name="portraitImage[order]" value="<%= portraitImage.order%>">
			</div>
			<input class="btn btn-lg btn-primary btn-block" type="submit" name="">
		</form>
	</div>
</div>
<% include ../partials/footer %>